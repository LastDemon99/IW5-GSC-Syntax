{
    "name": "lb-iw5-gsc",
    "displayName": "IW5-GSC-Syntax",
    "description": "GSC scripting syntax and autocomplete used for IW5.",
    "version": "0.0.1",
    "publisher": "LethalBeats",
    "icon": "images/iw5_gsc_syntax_logo_128x128.png",
    "repository": "https://github.com/LastDemon99/IW5-GSC-Syntax",
    "forked_repository": "https://github.com/SE2Dev/CoD-Sense",
    "engines": {
        "vscode": "^1.87.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [{
          "id": "gsc",
          "aliases": ["Game Script Code", "gsc", "GSC"],
          "extensions": [".gsc"],
          "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gsc",
            "scopeName": "source.gsc",
            "path": "./syntaxes/gsc.tmLanguage.json"
        }],
        "snippets": [
			{
				"language": "gsc",
				"path": "./snippets.json"
			}
		]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "vscode-languageclient": "^4.1.3"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.0.0",
        "@types/vscode": "^1.23.0",
        "tslint": "^5.8.0",
        "typescript": "^4.0.0"
    }
}