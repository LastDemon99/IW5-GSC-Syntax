{
    "name": "lb-iw5-gsc",
    "displayName": "IW5 GSC Syntax",
    "description": "GSC scripting syntax and autocomplete used for IW5.",
    "version": "1.5.1",
    "publisher": "LastDemon99",
    "company": "LethalBeats",
    "icon": "images/iw5_gsc_syntax_logo_128x128.png",
    "repository": "https://github.com/LastDemon99/IW5-GSC-Syntax",
    "forked_repository": "https://github.com/SE2Dev/CoD-Sense",
    "engines": {
        "vscode": "^1.87.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.downloadFiles",
                "title": "Download IW5 Scripts Files from GitHub"
            }
        ],
        "menus": {
            "editor/title": [
                {
                "command": "extension.downloadFiles",
                "when": "resourceExtname == .gsc",
                "group": "navigation"
                }
            ]
        },
        "languages": [
            {
                "id": "gsc",
                "aliases": [
                    "Game Script Code",
                    "gsc",
                    "GSC"
                ],
                "extensions": [
                    ".gsc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gsc",
                "scopeName": "source.gsc",
                "path": "./syntaxes/gsc.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "gsc",
                "path": "./snippets.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:gsc"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "axios": "^1.7.7",
        "chokidar": "^3.6.0",
        "vscode-languageclient": "^4.1.3"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.0.0",
        "@types/vscode": "^1.23.0",
        "tslint": "^5.8.0",
        "typescript": "^4.0.0"
    }
}
