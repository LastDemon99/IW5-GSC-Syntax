{
	"name": "cod-sense",
	"displayName": "CoD-Sense",
	"description": "Syntax highlighting and Intellisense support for Call of Duty's GSC and CSC scripts",
	"version": "0.0.2",
	"publisher": "SE2Dev",
	"engines": {
		"vscode": "^0.10.6"
	},
	"categories": [
		"Languages"
	],
	"activationEvents": [
		"onLanguage:gsc"
	],
	"main": "./out/src/extension",
	"contributes": {
		"languages": [{
			"id": "gsc",
			"aliases": ["GSC", "gsc"],
			"extensions": [".gsc", ".csc"],
			"configuration": "./gsc.configuration.json"
		}],
		"grammars": [{
			"language": "gsc",
			"scopeName": "source.gsc",
			"path": "./syntaxes/gsc.tmLanguage"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^1.7.5",
		"vscode": "^0.11.0"
	}
}