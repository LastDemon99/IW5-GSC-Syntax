{
    "name": "lb-iw5-gsc",
    "displayName": "IW5 GSC",
    "description": "GSC scripting syntax and autocomplete used for IW5.",
    "version": "1.5.6",
    "publisher": "LastDemon99",
    "company": "LethalBeats",
    "icon": "images/iw5_gsc_icon_128x128.png",
    "repository": "https://github.com/LastDemon99/IW5-GSC-Syntax",
    "forked_repository": "https://github.com/SE2Dev/CoD-Sense",
    "engines": {
        "vscode": "^1.87.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "iw5-gsc",
                    "title": "IW5 GSC",
                    "icon": "images/iw5_gsc_icon_bar_128x128.svg"
                }
            ]
        },
        "views": {
            "iw5-gsc": [
                {
                    "id": "iw5-gsc-explorer",
                    "name": "Scripts Explorer",
                    "visibility": "collapsed"
                }
            ]
        },
        "commands": [
            {
                "command": "iw5.scripts.download",
                "title": "IW5 GSC: Download Scripts from GitHub url",
                "icon": "$(link)"
            },
            {
                "command": "iw5.scripts.open",
                "title": "IW5 GSC: Open Scripts Folder",
                "icon": "$(folder)"
            },
            {
                "command": "iw5.scripts.explorer",
                "title": "IW5 GSC: Refresh Scripts Explorer",
                "icon": "$(extensions-refresh)"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "iw5.scripts.open",
                    "when": "view == iw5-gsc-explorer",
                    "group": "navigation"
                },
                {
                    "command": "iw5.scripts.download",
                    "when": "view == iw5-gsc-explorer",
                    "group": "navigation"
                },
                {
                    "command": "iw5.scripts.explorer",
                    "when": "view == iw5-gsc-explorer",
                    "group": "navigation"
                }
            ]
        },
        "languages": [
            {
                "id": "gsc",
                "aliases": [
                    "Game Script Code",
                    "gsc",
                    "GSC"
                ],
                "extensions": [
                    ".gsc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gsc",
                "scopeName": "source.gsc",
                "path": "./syntaxes/gsc.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "gsc",
                "path": "./snippets.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:gsc"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "@vscode/webview-ui-toolkit": "^1.4.0",
        "axios": "^1.7.7",
        "chokidar": "^3.6.0",
        "dotenv": "^16.4.5",
        "markdown-it": "^14.1.0",
        "vscode-languageclient": "^4.1.3"
    },
    "devDependencies": {
        "@types/markdown-it": "^14.1.2",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.66",
        "@types/vscode": "^1.23.0",
        "tslint": "^5.8.0",
        "typescript": "^4.0.0"
    }
}
