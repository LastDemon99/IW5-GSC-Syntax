	"scriptInit": {
		"prefix": "init",
		"body": [
            "init()",
            "{",
            "   level thread onPlayerConnect();",
            "}",
            "",
            "onPlayerConnect()",
            "{",
            "   level endon(\"game_ended\");",
            "",
            "   for (;;)",
            "   {",
            "      level waittill(\"connected\", player);",
            "      ",
            "      player thread onPlayerSpawn();",
            "   }",
            "}",
            "",
            "onPlayerSpawn()",
            "{",
            "   self endon(\"disconnect\");",
            "   ",
            "   for(;;)",
            "   {",
            "      self waittill(\"spawned_player\");",
            "      ",
            "   }",
            "}"
          ],
		"description": "Script init template"
	},

