{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "lib": [
            "es6"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
        "noUnusedLocals": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "server",
        "native",
        "src/defs/.vscode",
        "src/defs/build",
        "src/defs/raw",
        "research"
    ]
}
